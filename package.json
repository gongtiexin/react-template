    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-loadable": "^5.5.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/shortid": "^0.0.29",
    "@types/webpack-env": "^1.15.2",
    "antd": "^4.2.5",
    "axios": "^0.19.0",
    "echarts": "^4.7.0",
