{
  "name": "react-template",
  "version": "1.0.0",
  "description": "A frontend project template for React.js",
  "author": "tiexin.gong <tiexin.gong@qq.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js",
    "build": "rimraf ./dist && webpack --progress --config webpack.config.production.js",
    "loading": "rimraf ./dist/static/html/loading && webpack --config static/html/loading/webpack.loading.config.js",
    "prettier": "prettier --trailing-comma es5  --print-width 80 --write \"./**/*.js\" \"./**/*.less\" \"./**/*.json\" \"./**/*.md\"",
    "eslint": "eslint \"./**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "keywords": [
    "React",
    "React-Router-v4",
    "MobX",
    "Webpack",
    "ES6",
    "ES7"
  ],
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead",
    "not ie <= 8"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
