    "less": "^3.8.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^6.0.2",
