{
  "name": "react-template",
  "version": "1.0.0",
  "description": "A frontend project template for React.js",
  "author": "tiexin.gong <tiexin.gong@qq.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js",
    "build": "rimraf ./dist && webpack --progress --config webpack.config.production.js",
    "prettier": "prettier -c --write '**/*'"
  },
  "lint-staged": {
    "*": [
      "yarn prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "React",
    "React-Router-v4",
    "MobX",
    "Webpack",
    "TypeScript"
  ],
  "browserslist": [
    "> 1%",
    "last 1 version",
    "not dead",
    "not ie < 11"
  ],
  "license": "ISC",
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "file-loader": "^5.1.0",
